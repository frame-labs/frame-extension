{
  "name": "frame-extension",
  "version": "0.10.2",
  "repository": {
    "type": "git",
    "url": "git://github.com/floating/frame-extension.git"
  },
  "description": "Frame Companion Extension",
  "main": "src",
  "scripts": {
    "build": "rimraf ./dist && webpack --config webpack.config.js && node ./src/inline.js",
    "bundle": "webpack --config webpack.config.js",
    "setup": "npm install && husky install",
    "test": "jest",
    "watch": "npm run build && webpack --config webpack.dev.config.js",
    "serve": "webpack serve --config webpack.dev.layer.config.js",
    "lint": "ESLINT_USE_FLAT_CONFIG=true eslint -c eslint.config.mjs .",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=true eslint -c eslint.config.mjs --fix .",
    "format": "prettier --write '**/*.{yml,json,md,html,ts,tsx,mjs,js,jsx}'",
    "format:check": "prettier --check '**/*.{yml,json,md,html,ts,tsx,mjs,js,jsx}'"
  },
  "author": "Frame Labs, Inc.",
  "license": "",
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@babel/plugin-transform-runtime": "7.18.2",
    "@babel/preset-react": "7.17.12",
    "@framelabs/config": "file:../dev-tools/config",
    "eslint": "8.37.0",
    "eslint-config-prettier": "8.8.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.0",
    "ncp": "2.0.0",
    "node-polyfill-webpack-plugin": "1.1.4",
    "prettier": "2.8.7",
    "rimraf": "3.0.2",
    "terser-webpack-plugin": "5.3.3",
    "webpack": "5.73.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.9.2"
  },
  "dependencies": {
    "@babel/preset-env": "7.18.2",
    "@babel/runtime": "7.18.3",
    "@ensdomains/address-encoder": "0.2.17",
    "@ensdomains/content-hash": "2.5.7",
    "@ensdomains/resolver": "0.3.1",
    "@framelabs/pylon-client": "0.0.8",
    "babel-loader": "8.2.5",
    "content-hash": "2.5.2",
    "eth-ens-namehash": "2.0.8",
    "eth-provider": "0.13.5",
    "ethers": "5.6.8",
    "nebula": "0.1.3",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-restore": "0.5.0",
    "styled-components": "5.3.5"
  },
  "prettier": "@framelabs/config/prettier"
}
